<?php

function dumpstamp_menu() {
  $items['dumpstamp'] = array(
    'title' => 'Dump Stamp',
    'access callback' => TRUE,
    'page callback' => 'dumpstamp_page',
  );

  return $items;
}

function dumpstamp_page() {
  $message = t('A time stamp: ');
  
  $page = array(
    '#markup' => "$message" . date('M d, Y'),
  );

  return $page;
}
